<h1 class="headers">Sign Up </h1>
<div class="signup-login-review">
<p>Already have an account?<%= link_to "login", login_path %></p>
<%= form_for @user do |f| %>
<div class="input-group">
<%= f.label :name %>
<%= f.text_field :name %> <br>
<%= f.label :email %>
<%= f.email_field :email %> <br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.submit "Create Account", class: "submit-btn" %>
<button class="fb-button"><%= link_to "Google Login", login_path %> </button>
<% end %>
</div>
</div>